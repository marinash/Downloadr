{
    "manifest_version": 2,
    "default_locale": "en",
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "version": "3.0.26",
    "permissions": [
        {
            "fileSystem": [
                "write",
                "retainEntries",
                "directory"
            ]
        },
        "storage",
        "webview",
        "system.storage",
        "unlimitedStorage",
        "notifications",
        "fullscreen",
        "https://api.flickr.com/*",
        "https://*.staticflickr.com/*",
        "http://brain.no/*",
        "<all_urls>"
    ],
    "minimum_chrome_version": "31",
    "offline_enabled": true,
    "icons": {
        "16": "img/icon_16.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png",
        "256": "img/icon_256.png"
    },
    "app": {
        "background": {
            "scripts": [
                "scripts/main.js",
                "scripts/chromereload.js"
            ]
        }
    }
}
